<%= form_for :trip do |f|%>

    <%=f.label "Trip Name" %>
    <%=f.text_field :name %>
    <br>
    <%= f.label :start_date %>
    <%= f.date_field :start_date %>
    <%= f.label :end_date %>
    <%= f.date_field :end_date %>
    <br>


    <%= f.label :city %>
    <%= f.text_field :city%>

    <%= f.submit "Add Trip" %>
<%end%>
