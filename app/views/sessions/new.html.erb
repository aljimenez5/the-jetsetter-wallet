<% if flash[:notice] %>
  <div class="notice"><%= flash[:notice] %></div>
<% end %>

<h2> Jetsetter Log In </h2>
<%= form_for :user do |f|%>
    <p><%= f.label :email %>
    <%= f.text_field :email %></p>

    <p><%= f.label :password %>
    <%= f.text_field :password %></p>

    <p><%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %></p>

    <%= f.submit "Log In" %>

<%end%>