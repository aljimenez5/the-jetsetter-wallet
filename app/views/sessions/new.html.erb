<div class="log_in_container">
    <h2> Jetsetter Log In </h2>
    <%= form_for :user do |f|%>
        <p><%= f.label :email %>
        <%= f.text_field :email %></p>

        <p><%= f.label :password %>
        <%= f.text_field :password %></p>
        <br>
        <%= f.submit "Log In", class: "submit_button" %>

    <%end%>

    <br>
    <h3> -OR- </h3>
    <%= link_to 'Log in with Google', '/auth/google_oauth2', class: "google_link"%>
</div>